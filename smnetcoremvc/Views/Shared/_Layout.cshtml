                    <li><a class="nav-link scrollto" asp-area="" asp-controller="Projects" asp-action="Index">Проекты</a></li>
                    <li><a class="nav-link scrollto" asp-area="" asp-controller="Contact" asp-action="Index">Контакты</a></li>
                    <li><a class="getstarted scrollto" asp-area="" asp-controller="Resume" asp-action="Index">Резюме</a></li>
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav>
            @* .navbar *@

        </div>
    </header>
    @*End Header*@

    <main role="main" class="pb-3">
        @RenderBody()
